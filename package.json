{
  "name": "hono-file-based-router",
  "version": "0.1.40",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prepare": "tsup",
    "test": "bun typecheck && bun test:unit",
    "test:unit": "vitest --run ./src",
    "typecheck": "tsc --noEmit",
    "build": "tsup && publint",
    "watch": "tsup --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --check \"src/**/*.{js,ts}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts}\"",
    "prerelease": "bun run test && bun run build",
    "release": "np"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./factory": {
      "types": "./dist/factory/index.d.ts",
      "import": "./dist/factory/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./server/base": {
      "types": "./dist/server/base.d.ts",
      "import": "./dist/server/base.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types"
      ],
      "factory": [
        "./dist/factory"
      ],
      "server": [
        "./dist/server"
      ],
      "server/base": [
        "./dist/server/base"
      ]
    }
  },
  "author": "Yusuke Wada <yusuke@kamawada.com> (https://github.com/yusukebe)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yayugu/hono-file-based-router.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "jsonc-parser": "3.3.1",
    "precinct": "12.1.2",
    "tinyglobby": "^0.2.14"
  },
  "overrides": {
    "@typescript-eslint/typescript-estree": "^8.19.0"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^8.19.0"
  },
  "peerDependencies": {
    "hono": ">=4.*"
  },
  "devDependencies": {
    "@babel/generator": "7.25.6",
    "@babel/parser": "7.25.6",
    "@babel/traverse": "7.25.6",
    "@babel/types": "7.25.6",
    "@hono/vite-dev-server": "0.19.0",
    "@hono/eslint-config": "^1.1.1",
    "@mdx-js/rollup": "^3.0.0",
    "@playwright/test": "^1.42.0",
    "@types/node": "^20.10.5",
    "eslint": "^9.23.0",
    "glob": "^10.3.10",
    "happy-dom": "^15.11.6",
    "hono": "4.4.13",
    "np": "^10.2.0",
    "prettier": "^3.1.1",
    "publint": "^0.2.7",
    "tsup": "^8.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.12",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.3"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  }
}
